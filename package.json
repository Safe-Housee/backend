{
  "name": "backend",
  "version": "1.0.0",
  "description": "A API for SafeHouse project",
  "main": "app.js",
  "scripts": {
    "test": "docker-compose up -d && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all && node src/database/clean.js && jest --forceExit",
    "prd": "sucrase ./src -d ./dist --transforms imports && node dist/app.js",
    "dev": "docker-compose up -d && nodemon src/app.js",
    "clean": "docker-compose up -d && npx sequelize db:migrate:undo:all",
    "reset": "docker-compose up -d && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all ",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "test:staged": "docker-compose up -d && npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all && node src/database/clean.js && jest --passWithNoTests --forceExit",
    "test:docker": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all && node src/database/clean.js && jest --forceExit",
    "migrate": "npx sequelize db:migrate && npx sequelize db:seed",
    "start": "nodemon src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Safe-Housee/backend.git"
  },
  "jest": {
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Safe-Housee/backend/issues"
  },
  "engines": {
    "node": "14"
  },
  "homepage": "https://github.com/Safe-Housee/backend#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "image-to-base64": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.5.4",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^4.1.2",
    "sucrase": "^3.17.0",
    "supertest": "^6.1.3",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/sequelize": "^4.28.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  }
}
