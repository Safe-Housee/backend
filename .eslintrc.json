{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "import/prefer-default-export": "off",
    "no-undef": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-shadow": "off"
  }
}
